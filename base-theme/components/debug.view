	.websom-color-bubble-container {
		position: relative;
		height: 75px;

		border: 2px solid transparent;

		transition: .3s;

		.websom-color-bubble {
			position: absolute;
			width: 50px;
			height: 50px;

			top: calc(50% - 25px);
			left: calc(50% - 25px);
			border-radius: 50%;

			transition: .3s;
		}

		.websom-mini-color-bubble {
			position: absolute;
			width: 20px;
			height: 20px;

			top: calc(50% + 5px);
			left: calc(50% + 5px);
			border-radius: 50%;

			transition: .3s;
		}

		&:hover {
			z-index: 12;
			border-radius: 10px;
			transform: scale(2);
			background: var(--color);

			border: 2px solid var(--contrast);

			.websom-color-bubble {
				width: 30px;
				height: 30px;

