script {

import rootStylesheet from "raw-loader!../styles/root.view?extract-style=true";

export default {
	data() {
		let fivePointBaseDistance = 5;

		let genHandle = (label, amount) => {
			return {
				label: label,
				relative: {
					hue: 0,
					saturation: 0
				},
				global: {
					hue: Math.sign(amount) * ((Math.abs(amount) >= fivePointBaseDistance * 2) ? 2 : 1),
					saturation: ((Math.abs(amount) >= fivePointBaseDistance * 2) ? 2 : 1)
				},
				control: Math.abs(amount) < fivePointBaseDistance * 2,
				driver(basis) {
					return {
						hue: basis.hue + amount,
						saturation: basis.saturation,
						luminosity: basis.luminosity
					};
				}
			};
		};

		let colors = [
			[210, 95, 50],
			[39.790824167816993, 97.2276204429955, 50]
		];

		let randomColor = colors[Math.floor(colors.length * Math.random())];
		
		return {
			pack: "material",
			packs: ["material", "font-awesome", "remix"],
			index: 0,
			rangeValueSingle: [50],
			rangeValueDouble: [25, 75],
			rangeValueTriple: [0, 50, 100],
			rangeValueCenter: [0.5],
			rangeValueNegative: [-100, 0],
			rangeValueMax: [100],
			reverse: [0],
			color: {
				mode: "fivePoint",
				modes: {
					fivePoint: { // Five point
						name: "Five Point",
						handles: [
							genHandle("side-r-1", fivePointBaseDistance),
							genHandle("side-r-2", fivePointBaseDistance * 2),
							genHandle("side-l-1", -fivePointBaseDistance),
							genHandle("side-l-2", -fivePointBaseDistance * 2),
							{
								label: "basis",
								value: randomColor
							}
						]
					}
				}
			},
			wheelValues: {},
			primary: "#FFFFFF",
			secondary: "#FFFFFF",
			tertiary: "#FFFFFF",
			customCSS: "",
			slideOut: {
				edge: "left",
				size: "normal",
				close: false
			}
		};
	},
	mounted() {
		setInterval(() => {
			this.index++;
			if (this.index >= this.packs.length)
				this.index = 0;

			this.pack = this.packs[this.index];

			//this.$set(this.colorSelectedHandles[0].value, 0, this.colorSelectedHandles[0].value[0] + 10);
			
		}, 2000);

		this.generateColors();
	},
	computed: {
		colorSelectedHandles() {
			return this.color.modes[this.color.mode].handles;
		}
	},
	created() {
		import("less");
	},
	methods: {
		async generateColors() {
			if (typeof less === "undefined") { // Wait until less is loaded
				setTimeout(() => {this.generateColors()}, 100);
				return;
			}

			let time = Date.now();
			this.customCSS = (await less.render(rootStylesheet, {modifyVars: {
				primary: this.primary,
				secondary: this.secondary,
				tertiary: this.tertiary,
				important: '~"html"'
			}})).css;
			console.log("Color compilation took ", Date.now() - time);
		},
		openSlideOut(edge, size) {
			this.slideOut.edge = edge;
			this.slideOut.size = size;

			if (size == "full" || size == "max")
				this.slideOut.close = true;
			else
				this.slideOut.close = false;

			// Wait for shift calculations after sizing change
			setTimeout(() => {
				this.$refs.slideOut.open();
			}, 10);
		},
		openModal() {
			this.$refs.modal.open();
		}
	},
	watch: {
		wheelValues() {
			this.primary = this.wheelValues.basis;
			this.secondary = this.wheelValues["side-r-2"];
			this.tertiary = this.wheelValues["side-l-2"];
			
			this.generateColors();
		}
	}
}

}

